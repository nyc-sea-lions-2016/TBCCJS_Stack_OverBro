<div class="answer" id="<%=answer.id%>">
  <% if logged_in? %>
  <%= erb :'/answers/_answer_vote_partial', layout: false, locals: {answer: answer} %>
  <% end %>
  <p><%=answer.content %></p>
  <p><%=answer.user.username%> | <%=answer.updated_at%></p>
  <span> <%=answer.points %> points </span>
  <br>
</div>
